= form_tag sessions_path, role: 'form' do
  .form-group
    = label_tag 'email'
    = text_field_tag 'email', params[:email], class: 'form-control'
  .form-group
    = label_tag 'password'
    = password_field_tag 'password', nil, class: 'form-control'
  = submit_tag 'Log in', class: 'btn btn-info'
